[Lasso_Comment]<fusedoc fuse="FBX_Switch.lasso">	<responsibilities>		I am the cfswitch statement that handles the fuseaction, delegating work to various fuses.	</responsibilities>	<properties>		<history author="Michael McKellip" date="1/5/2003" email="mmckellip@lassofusebox.org" type="create"></history>	</properties>	<io>		<in>			<structure name="fusebox">				<string name="fuseaction" />				<string name="circuit" />				</structure>		</in>	</io>	</fusedoc>[/Lasso_Comment][Encode_Set: -EncodeNone][Select:$fuseaction]	[Case:'faq']		[FBX_Include:'qry_getFaqs.lasso']		[FBX_Include:'dsp_listFaqs.lasso']	[Case:'FLiP']		[Variable:'xfa_personas'='resources.personas']		[Variable:'xfa_wireframes'='resources.wireframes']		[Variable:'xfa_prototyping'='resources.prototyping']		[Variable:'xfa_architecting'='resources.architecting']		[Variable:'xfa_fusecoding'='resources.fusecoding']		[Variable:'xfa_unittesting'='resources.unittesting']		[FBX_Include:'dsp_FLiP.lasso']	[Case:'personas']		[Variable:'xfa_wireframes'='resources.wireframes']		[FBX_Include:'dsp_personas.lasso']	[Case:'wireframes']		[Variable:'xfa_personas'='resources.personas']		[Variable:'xfa_prototyping'='resources.prototyping']		[FBX_Include:'dsp_wireframes.lasso']	[Case:'prototyping']		[Variable:'xfa_architecting'='resources.architecting']		[FBX_Include:'dsp_prototyping.lasso']	[Case:'architecting']		[Variable:'xfa_fusecoding'='resources.fusecoding']		[FBX_Include:'dsp_architecting.lasso']	[Case:'fusecoding']		[Variable:'xfa_unittesting'='resources.unittesting']		[FBX_Include:'dsp_fusecoding.lasso']	[Case:'unittesting']		[FBX_Include:'dsp_unittesting.lasso']	[Case:'samples']		[FBX_Include:'qry_getSamples.lasso']		[Variable:'xfa_downloads'='downloads.list']		[FBX_Include:'dsp_samples.lasso']	[Case:'whitepapers']		[FBX_Include:'qry_getWhitePapers.lasso']		[FBX_Include:'dsp_whitepapers.lasso']	[Case]		<p>This is the default case tag. I received a fuseaction called "[$fuseaction]" and I don't know what to do with it.</p>[/Select][/Encode_Set]