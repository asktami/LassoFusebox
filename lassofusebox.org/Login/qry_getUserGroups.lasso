[FBX_Param: -Name='getLogin.Found_Count', -Default='-1'][FBX_Param: -Name='getLogin.ID', -Default='-1'][Variable:'sql' = '	SELECT Groups.ID, Groups.GroupName	FROM Groups, UserGroups	WHERE Groups.ID = UserGroups.GroupID	AND UserGroups.UserID = ' + ($getLogin.ID)][Inline: -Search,	-Username=($FBX_username),	-Password=($FBX_password),    -Database=($datasource),    -SQL=($sql),	-InlineName='getUserGroups']		[FBX_Param: -Name='getUserGroups.Found_Count', -Default=(Found_Count)]	[/Inline]