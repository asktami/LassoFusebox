[Variable:'ErrMsg' = (Array)][Inline: -Search,	-Username=($FBX_username),	-Password=($FBX_password),    -Database=($datasource),    -Table='Users',    -KeyField='ID',    'Email'=($email),	'Password'=($password),	-InlineName='getLogin']		[FBX_Param: -Name='getLogin.ID', -Default=(Field:'ID')]	[FBX_Param: -Name='getLogin.FirstName', -Default=(Field:'FirstName')]	[FBX_Param: -Name='getLogin.Found_Count', -Default=(Found_Count)]	[/Inline][If:($getLogin.Found_Count) == 0][$ErrMsg->(Insert: 'User information not found.')]<h1>Error</h1><table>	[Iterate: $ErrMsg, (Variable: 'temp')]	<tr>		<td>[Output: $temp]</td>	</tr>	[/Iterate]	<tr>		<td><p>Please use your back button to try again.</p></td>	</tr></table>	[/If]