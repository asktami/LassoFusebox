[If:!(Lasso_TagExists:'FBX_Param')]	[Define_Tag: 'FBX_Param', -Required='Name', -Required='Default']  		[If: !(Variable_Defined: #Name)][Variable: #Name = #Default][/If]	[/Define_Tag][/If][If:!(Lasso_TagExists:'FBX_SaveContent')]	[Define_Tag:'FBX_SaveContent', -Container, -Required = 'Variable']		[Variable_Set:#Variable = (Run_Children)]	[/Define_Tag][/If][If:!(Lasso_TagExists:'FBX_Include')]	[Define_Tag: 'FBX_Include']		[FBX_SaveContent: -Variable="_temp"]		[Include: ($fusebox->(Find:'currentpath') + (Params)->get:1)]		[/FBX_SaveContent]		[Return:$_temp]	[/Define_Tag][/If][If:!(Lasso_TagExists:'FBX_Redirect')][Define_Tag:'fbx_redirect']<!-- [FBX_Redirect]DESCRIPTION:Use this tag instead of [redirect_url] within your LassoFusebox applications. If the -UseLink parameter is specified it will add all session ID's to the redirect.Note that if you don't have any active sessions, you can still safely use this tag, it will work exactly like [redirect_url].  Also note that if you do have sessions and you're redirecting to outside your website, be sure to either use [redirect_url] instead, or know that you'll be passing the session IDs too.Original code and concept is courtesy of Bil Corry <bil@bilcorry.com>USE:[fbx_redirect:'http://www.somedomainname.com/']or[fbx_redirect:'http://www.somedomainname.com/', -Uselink]-->[Local:'site'=params->(get:1)][If:(Local_Defined:'-UseLink')]		[Local:'symbol' = '?']	<!-- does the URL already have parameters? -->	[If:#site->(contains:'?')]		[#symbol = '&']	[/If]	<!-- small speed trick, copy [vars] to a local variable -->	[Local:'vars'=vars]	<!-- find all the sessions contained in [vars] -->	[Local:'sessions' = '']	[Loop:#vars->size]		[Local:'varname' = #vars->(get:loop_count)->first]		[Local:'varvalue' = #vars->(get:loop_count)->second]			[If:#varname->type=='String' && #varname->(BeginsWith:'SessionTracker') && !#varname->(EndsWith:'_Vars')]			<!-- extract the session name -->			[local:'session_name' = #varname->(Remove_Leading:'_SessionTracker_')]			<!-- add to sessions -->			[#sessions += (#symbol + '-session=' + (#session_name) + ':' + (#varvalue))]			<!-- from now on, we know it will be an additional parameter to the URL -->			[#symbol = '&']		[/If]	[/Loop]	<!-- add the session URL onto the original URL -->	[#site += #sessions][/If]<!-- redirect to the URL -->[Redirect_Url:#site][/Define_Tag][/If]