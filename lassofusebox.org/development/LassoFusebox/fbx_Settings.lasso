[Lasso_Comment]<fusedoc fuse="FBX_Settings.lasso">	<responsibilities>		I set up the enviroment settings for this circuit. If this settings file is being inherited, then you can use CFSET to override		a value set in a parent circuit or CFPARAM to accept a value set by a parent circuit	</responsibilities>		<properties>		<history author="Michael McKellip" email="mmckellip@lassofusebox.org" date="1/5/2003" type="create">	</properties>	<io>		<in>			<structure name="fusebox">				<boolean name="isHomeCircuit" comments="Is the circuit currently executing the home circuit?" />			</structure>		</in>		<out>			<string name="self" scope="variables" />			<string name="fuseaction" scope="variables" />			<string name="datasource" scope="variables" />			<boolean name="fusebox.suppressErrors" />		</out>	</io></fusedoc>[/Lasso_Comment][FBX_Param: -Name='fuseaction', -Default='home.welcome'][FBX_Param: -Name='self', -Default='index.lasso'][FBX_Param: -Name='datasource', -Default='LFB_database1'][FBX_Param: -Name='EmailHost', -Default='mail.lassofusebox.org'][FBX_Param: -Name='EmailAddress', -Default='michael@lassofusebox.org'][FBX_Param: -Name='AltEmailAddress', -Default='rich@lassofusebox.org'][FBX_Param: -Name='FBX_username', -Default='LFB_lassofusebox'][FBX_Param: -Name='FBX_password', -Default='xobesufossal'][$fusebox->(Insert:'suppressErrors'=false)][Session_Start: -Name='LFB3', -Expires=1440, -UseCookie][Session_AddVariable: -Name='LFB3', 'Session_ID'][Session_AddVariable: -Name='LFB3', 'Session_FirstName'][FBX_Param: -Name='Session_ID', -Default=0][FBX_Param: -Name='Session_FirstName', -Default=''][FBX_Param: -Name='isLoggedIn', -Default=false][If:($Session_ID) > 0]	[$isLoggedIn = true][/If]