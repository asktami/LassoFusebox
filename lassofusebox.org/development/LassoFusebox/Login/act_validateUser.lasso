[FBX_Param: -Name='validationOK', -Default='False'][Variable:'ErrMsg' = (Array)][If:!(Variable_Defined:'firstname') || ($firstname->size) == 0]	[$ErrMsg->(Insert: 'First Name is required.')][/If][If:!(Variable_Defined:'lastname') || ($lastname->size) == 0]	[$ErrMsg->(Insert: 'Last Name is required.')][/If][If:!(Variable_Defined:'email') || ($email->size) == 0]	[$ErrMsg->(Insert: 'Email address is required.')][/If][If:!(Valid_Email:($email))]	[$ErrMsg->(Insert: 'Email address submitted is not a valid email address.')][/If][If:!(Variable_Defined:'password') || ($password->size) == 0]	[$ErrMsg->(Insert: 'Password is required.')][/If][If:($password->size) < 4 || ($password->size) > 16]	[$ErrMsg->(Insert: 'Password must be between 4 and 16 characters in length.')][/If][If:!(Variable_Defined:'hint') || ($hint->size) == 0]	[$ErrMsg->(Insert: 'Please choose a hint.')][/If][If:!(Variable_Defined:'answer') || ($answer->size) == 0]	[$ErrMsg->(Insert: 'Please supply an answer to your hint.')][/If][If:($ErrMsg->Size) == 0]	[$validationOK = 'True'][Else]<h1>Error</h1><table>	[Iterate: $ErrMsg, (Variable: 'temp')]	<tr>		<td>[Output: $temp]</td>	</tr>	[/Iterate]	<tr>		<td><p>Please use your back button and make the corrections.</p></td>	</tr></table>	[/If]