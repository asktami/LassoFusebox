[Lasso_Comment]<fusedoc fuse="FBX_Switch.lasso">	<responsibilities>		I am the cfswitch statement that handles the fuseaction, delegating work to various fuses.	</responsibilities>	<properties>		<history author="Michael McKellip" date="1/5/2003" email="mmckellip@lassofusebox.org" type="create"></history>	</properties>	<io>		<in>			<structure name="fusebox">				<string name="fuseaction" />				<string name="circuit" />				</structure>		</in>	</io>	</fusedoc>[/Lasso_Comment][Encode_Set: -EncodeNone][Select:$fusebox->(Find:'fuseaction')]	[Case:'login']		[Variable:'xfa_register'='login.register']		[Variable:'xfa_forgotpassword'='login.forgotpassword']		[Variable:'xfa_login'='login.authorize']		[FBX_Include:'dsp_login.lasso']			[Case:'authorize']		[FBX_Include:'act_validateLogin.lasso']		[If:($validationOK)]			[FBX_Include:'qry_getLogin.lasso']			[If:($getLogin.Found_Count) == 1]				[FBX_Include:'qry_getUserGroups.lasso']				[FBX_Include:'act_setPermissions.lasso']				[Variable:'xfa_redirect'='home.welcome']				[FBX_Include:'url_redirect.lasso']			[/If]		[/If]			[Case:'register']		[FBX_Include:'qry_getUser.lasso']		[Variable:'xfa_submit'='login.adduser']		[FBX_Include:'dsp_user.lasso']			[Case:'adduser']		[FBX_Include:'act_validateUser.lasso']		[If:$validationOK]			[FBX_Include:'qry_addUser.lasso']			[Variable:'xfa_redirect'='home.welcome']			[FBX_Include:'url_redirect.lasso']		[/If]			[Case:'myprofile']		[FBX_Include:'qry_getUser.lasso']		[Variable:'xfa_submit'='login.updateuser']		[FBX_Include:'dsp_user.lasso']			[Case:'updateuser']		[FBX_Include:'act_validateUser.lasso']		[If:$validationOK]			[FBX_Include:'qry_updateUser.lasso']			[Variable:'xfa_redirect'='home.welcome']			[FBX_Include:'url_redirect.lasso']		[/If]			[Case:'forgotpassword']		[Variable:'xfa_showhint'='login.showhint']		[Variable:'xfa_emailpassword'='login.emailpassword']		[FBX_Include:'dsp_forgotpassword.lasso']		[Case:'showhint']		[FBX_Include:'qry_getHint.lasso']		[If:($getHint.Found_Count)==1]			[Variable:'xfa_displaypassword'='login.displaypassword']			[FBX_Include:'dsp_showHint.lasso']		[Else]			[FBX_Include:'dsp_emailNotFound.lasso']		[/if]		[Case:'displaypassword']		[FBX_Include:'qry_getPasswordByHint.lasso']		[If:($getPasswordByHint.Found_Count) == 1]			[FBX_Include:'dsp_showPassword.lasso']		[Else]			[FBX_Include:'dsp_wrongAnswer.lasso']		[/If]			[Case:'emailpassword']		[FBX_Include:'qry_getPasswordByEmail.lasso']		[If:($getPasswordByHint.Found_Count) == 1]			[FBX_Include:'act_emailPassword.lasso']			[FBX_Include:'dsp_emailPassword.lasso']		[Else]			[FBX_Include:'dsp_emailNotFound.lasso']		[/If]		[Case:'logout']		[FBX_Include:'act_logout.lasso']		[Variable:'xfa_redirect'='home.welcome']		[FBX_Include:'url_redirect.lasso']			[Case]		<p>This is the default case tag. I received a fuseaction called "[$fuseaction]" and I don't know what to do with it.</p>[/Select][/Encode_Set]