[If:($isLoggedIn)]<h1>My Profile</h1>[Else]<h1>Registration</h1>[/If]<form action="[$self]?fuseaction=[$xfa_submit]" method="post" name="frmLogin"><input type="hidden" name="ID" value="[$getUser.ID]"><table align="center" width="500" cellpadding="4" cellspacing="0"><tr valign="middle">	<td align="left" width="100">First Name:</td>	<td align="right"><input type="text" name="FirstName" size="50" maxlength="30" value="[$getUser.FirstName]"></td></tr><tr valign="middle">	<td align="left" width="100">Last Name:</td>	<td align="right"><input type="text" name="LastName" size="50" maxlength="30" value="[$getUser.LastName]"></td></tr><tr valign="middle">	<td align="left" width="100">Email:</td>	<td align="right"><input type="text" name="Email" size="50" maxlength="50" value="[$getUser.Email]"></td></tr><tr valign="middle">	<td align="left">Password:</td>	<td align="right"><input type="password" name="Password" size="50" maxlength="16" value="[$getUser.Password]"></td></tr><tr valign="middle">	<td align="left">Hint:</td>	<td align="right">	<select name="Hint">	[Inline: -Show, -Username=($FBX_username), -Password=($FBX_password), -Database=($datasource), -Table='Users']	[Value_List:'Hint']	<option value="[Value_ListItem]"[If:($getUser.Hint) == (Value_ListItem)] SELECTED[/If]>[Value_ListItem]</option>	[/Value_List]	[/Inline]	</select>	</td></tr><tr valign="middle">	<td align="left" width="100">Answer:</td>	<td align="right"><input type="text" name="Answer" size="50" maxlength="30" value="[$getUser.Answer]"></td></tr><tr valign="middle">	<td align="left" colspan="2">Would you like to be notified of LassoFusebox news?:&nbsp;<input type="radio" name="MailList" value="1"[If:($getUser.MailList)] CHECKED[/If]>&nbsp;Yes&nbsp;<input type="radio" name="MailList" value="0"[If:!($getUser.MailList)] CHECKED[/If]>&nbsp;No</td></tr><tr>	<td colspan="2">&nbsp;</td></tr><tr>	<td colspan="2" align="right"><input type="submit" value="&nbsp;&nbsp;Submit&nbsp;&nbsp;"></td></tr></table></form>